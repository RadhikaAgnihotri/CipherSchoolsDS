package primitiveDT;
class Car
{
	String name;
	String color;
	int manYear;
	void print(){
		System.out.println("Name: "+name+", Color: "+color+", Year: "+manYear);
	}
}
public class constructor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car c = new Car();
		c.print();
		c.name = "Audi";
		c.color = "Orange";
		c.manYear = "2018";
		

	}

}
