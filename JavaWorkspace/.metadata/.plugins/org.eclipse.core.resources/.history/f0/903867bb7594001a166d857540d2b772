package primitiveDT;

public class first {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		byte b = 5; // assigning value 5 to a byte variable
		// = is called assignment operator
		int n=100;
//		int n=0.5; shows an error
		double d = 10.5;
		char c = 'a'; //always put characters in single quotes
		System.out.println(b);
		System.out.println(n);
		System.out.println(d);
		System.out.print(c);
	}

}
