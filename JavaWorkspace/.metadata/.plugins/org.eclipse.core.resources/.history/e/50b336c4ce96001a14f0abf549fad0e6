package primitiveDT;
class Coders{
	String name;
	int numId;
	int counter=0;
	Coders(String name, int numId) {
		this.name = name;
		this.numId = numId;
		counter++;
	}
	
}
public class StaticCount {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Coders c1 = new Coders("Shivam",1);
		Coders c2 = new Coders("Radhika",2);
		
		
	}

}
