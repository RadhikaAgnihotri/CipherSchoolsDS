package sorting;

public class bubblesort {
	static void print(int arr[])
	{
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]+" ");
		}
	}
	static void selsort(int arr[])
	{
		int n = arr.length;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = {10,7,9,2,4};
		bubblesort(arr);
		print(arr);
	}

}
